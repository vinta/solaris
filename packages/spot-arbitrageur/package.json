{
  "name": "@solaris/spot-arbitrageur",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "typegen:core": "typechain --target ethers-v6 --out-dir types/ '../../out/**/*.json'",
    "typegen": "npm run typegen:core",
    "deploy": "sls deploy -s prod --verbose",
    "deploy:base": "sls deploy function -s prod --function base",
    "invoke:prod:base": "sls invoke -s prod --function base --log",
    "invoke:local:base": "sls invoke local -s dev --function base",
    "logs:prod:base": "awslogs get -w --aws-region=us-west-1 /aws/lambda/spot-arbitrageur-prod-base --start=\"5m\"",
    "clean": "rm -rf .esbuild/ .serverless/ types/"
  },
  "dependencies": {
    "@sentry/serverless": "7.80.1",
    "async-mutex": "0.4.0",
    "ethers": "6.8.0"
  },
  "devDependencies": {
    "@typechain/ethers-v6": "0.5.1",
    "@types/aws-lambda": "8.10.125",
    "esbuild": "0.19.5",
    "serverless": "3.35.2",
    "serverless-deployment-bucket": "1.6.0",
    "serverless-esbuild": "1.48.5",
    "typechain": "8.3.2"
  }
}
